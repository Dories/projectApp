<?xml version="1.0" encoding="gbk"?>
<validators>
     
    <!-- 生日验证器 -->
    <field name="startBir">
    	<rules>
    		<rule><![CDATA[startBir > endBir]]></rule>
    		<message><![CDATA[客户起始出生日期晚于结束出生日期，请重新确认]]></message>
    	</rules> 
    </field>
   
     <!-- 年龄验证器 -->
     <field name="endAge">
     	<rules>
    		<rule><![CDATA[startAge > endAge]]></rule>
    		<message><![CDATA[客户起始年龄 不能大于结束年龄，请重新确认]]></message>
    	</rules> 
    </field>
    
     <!--收入验证器 -->
     <field name="startIncome">
     	<rules>
    		<rule><![CDATA[startIncome > endIncome]]></rule>
    		<message><![CDATA[客户起始收入不能大于结束收入，请重新确认]]></message>
    	</rules> 
    </field>
</validators>